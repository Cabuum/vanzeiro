<% unless current_page?(contact_path) %>
    <div class="section black">
      <div class="container">
        <div class="p-t-60 p-b-60">
          <div class="row">
            <div class="col-md-8 col-md-offset-2">
              <h2 class="text-center text-white m-b-30">Quer ficar por dentro das <span class="semi-bold">novidades?</span><br />
                Coloque seu email abaixo e surpreenda-se!</h2>
              <%= form_for(@newsletter, url: { controller: :newsletters, action: 'create', method: :post}, remote: true,
                           html: {id: 'form_newsletter'}) do |f| %>
                  <% if @newsletter.errors.any? %>
                      <div class="alert alert-block alert-error fade in col-md-10">
                        <button type="button" class="close" data-dismiss="alert"></button>
                        <h4 class="alert-heading"><i class="icon-warning-sign"></i> Atenção!</h4>
                        A sua tentativa gerou <a href="#" class="link">
                        <%= pluralize(@newsletter.errors.count,'erro') %>: </a>
                        <% @newsletter.errors.full_messages.each do |message| %>
                            <li><%= message %></li>
                        <% end %>
                      </div>
                  <% end %>

                  <div class="row form-row">
                    <div class="col-md-6 col-md-offset-2 no-padding  col-sm-6 col-sm-offset-2 col-xs-10 col-xs-offset-1">
                      <%= f.email_field :mail, class: 'form-control', placeholder: 'Seu email vem aqui!', required: true %>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-4 xs-p-l-10">
                      <%= f.submit 'Inscreva-me!', id: 'submit-newsletter', class: 'btn btn-primary btn-cons', data: { disable_with: 'Please wait...' } %>
                    </div>
                  </div>
                  <div class="row">
                    <a href="#" class="text-white text-center col-md-11 col-sm-11 col-xs-11">Fique tranquilo(a), também odiamos SPAM!</a>
                  </div>
                  <div class="clearfix"></div>
                  <div class="row m-t-30">
                    <div class="col-md-5 col-md-offset-2 col-sm-5 col-sm-offset-2">
                      <div class="checkbox check-default">
                        <%= f.check_box :skills %>
                        <label for="newsletter_skills">Dicas & Novidades sobre o vanzeiro!</label>
                      </div>
                    </div>
                    <div class="col-md-4 col-sm-4">
                      <div class="checkbox check-default">
                        <%= f.check_box :discounts %>
                        <label for="newsletter_skills">Descontos Exclusivos!</label>
                      </div>
                    </div>
                  </div>
              <% end %>
              <div id="message-newsletter" class="col-md-12">
                <div class="alert alert-success">
                  <button class="close" id="closer-button" data-dismiss="alert">&times;</button>
                  <%= t('newsletter.create.success') %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
<% end %>
