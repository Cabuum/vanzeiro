<% if current_page?(controller: 'devise/sessions', action: 'new') || current_page?(entrar_path) %>

    <!-- BEGIN CORE JS FRAMEWORK-->
    <%= javascript_include_tag '/assets/jquery-1.8.3.min',
                               '/assets/boostrapv3/bootstrap.min',
                               '/assets/pace/pace.min',
                               '/assets/jquery-validation/jquery.validate.min',
                               '/assets/jquery-lazyload/jquery.lazyload.min',
                               '/assets/jquery-ui/jquery-ui-1.10.1.custom.min',
                               '/assets/breakpoints',
                               '/assets/jquery-unveil/jquery.unveil.min',
                               '/assets/jquery-block-ui/jqueryblockui',
                               '/assets/devise.sessions.new' %>
    <!-- END CORE JS FRAMEWORK -->

    <%= javascript_include_tag 'application' %>
<% elsif current_user.blank? %>
    <%= javascript_include_tag '/assets/front-end/jquery-1.8.3.min' %>

    <!-- SLIDER REVOLUTION 4.x SCRIPTS  -->
    <%= javascript_include_tag '/assets/front-end/rs-plugin/jquery.themepunch.plugins.min',
                               '/assets/front-end/rs-plugin/jquery.themepunch.revolution.min' %>

    <!-- BEGIN CORE JS FRAMEWORK-->
    <%= javascript_include_tag '/assets/front-end/boostrapv3/bootstrap.min',
                               '/assets/front-end/pace/pace.min',
                               '/assets/front-end/jquery-unveil/jquery.unveil.min',
                               '/assets/front-end/owl-carousel/owl.carousel.min',
                               '/assets/front-end/waypoints.min',
                               '/assets/front-end/parrallax/jquery.parallax-1.1.3',
                               '/assets/front-end/jquery-nicescroll/jquery.nicescroll.min',
                               '/assets/front-end/jquery-appear/jquery.appear',
                               '/assets/front-end/jquery-numberAnimate/jquery.animateNumbers',
                               '/assets/front-end/core' %>
    <!-- END CORE JS FRAMEWORK -->

    <%= javascript_include_tag 'application' %>
<% else %>

    <!-- BEGIN CORE JS FRAMEWORK-->
    <%= javascript_include_tag '/assets/jquery-1.8.3.min',
                               '/assets/jquery-ui/jquery-ui-1.10.1.custom.min',
                               '/assets/boostrapv3/bootstrap.min',
                               '/assets/breakpoints',
                               '/assets/jquery-unveil/jquery.unveil.min',
                               'http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.3/underscore-min.js',
                               'http://cdnjs.cloudflare.com/ajax/libs/backbone.js/0.9.10/backbone-min.js' %>
    <!-- END CORE JS FRAMEWORK -->

    <!-- BEGIN PAGE LEVEL JS -->
    <%= javascript_include_tag  '/assets/jquery-scrollbar/jquery.scrollbar.min',
                                '/assets/jquery-block-ui/jqueryblockui',
                                '/assets/jquery-numberAnimate/jquery.animateNumbers',
                                '/assets/pace/pace.min',
                                '/assets/bootstrap-datepicker/bootstrap-datepicker',
                                '/assets/bootstrap-select2/select2.min',
                                '/assets/jquery-validation/jquery.validate.min',
                                '/assets/jquery-inputmask/jquery.inputmask.min',
                                '/assets/jquery-lazyload/jquery.lazyload.min',
                                '/assets/jquery-datatable/jquery.dataTables.min',
                                '/assets/jquery-datatable/dataTables.tableTools.min',
                                '/assets/datatables-responsive/datatables.responsive',
                                '/assets/datatables-responsive/lodash.min',
                                '/assets/jquery-notifications/messenger.min',
                                '/assets/jquery-notifications/messenger-theme-future',
                                '/assets/jquery-notifications/location-sel',
                                '/assets/jquery-notifications/theme-sel',
                                '/assets/jquery-notifications/demo',
                                '/assets/jquery-ricksaw-chart/d3.v2.min',
                                '/assets/jquery-ricksaw-chart/raphael-min',
                                '/assets/jquery-ricksaw-chart/rickshaw.min',
                                '/assets/jquery-sparkline/jquery-sparkline' %>

    <!-- END PAGE LEVEL PLUGINS -->

    <%= javascript_include_tag '/assets/datatables',
                               '/assets/form_validations',
                               '/assets/form_elements',
                               '/assets/notifications' %>

    <!-- BEGIN CORE TEMPLATE JS -->
    <%= javascript_include_tag '/assets/core',
                               '/assets/chat',
                               '/assets/demo' %>
    <!-- END CORE TEMPLATE JS -->

    <%= javascript_include_tag 'application' %>
<% end %>