          <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right" >
              <li><%= link_to 'Home', root_path %></li>
              <li><%= link_to 'Entrar', entrar_path %></li>
              <li><%= link_to 'Planos', pricing_path %></li>
              <li><%= link_to 'Contato', contato_path %>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>
<% end %>
